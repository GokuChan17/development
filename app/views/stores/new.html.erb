<div class="booyah-box col-10 offset-1">
  <h1>Enter Your Store Details</h1>
  <%= simple_form_for @store do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :address %>
    <br />
    <%= f.submit 'Create', class: 'btn btn-primary'  %>
    <%= link_to 'Cencel', root_path, class: 'btn btn-primary'  %>
  <% end %>
</div>